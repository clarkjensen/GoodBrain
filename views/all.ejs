<% layout('layouts/boilerplate')%>
    <div class="row">
        <h1 class="text-center">Good things you've said about yourself:</h1>
        <div class="col-6 offset-3">
            <% for(let thing of goodThings) { if(!(thing.date===dateCurrentlyPrinting)) {
                dateCurrentlyPrinting=thing.date; %>
                <h3>
                    <%= dateCurrentlyPrinting %>
                </h3>
                <%}%>

                    <h5>
                        <%= thing.text %>
                    </h5>
                    <% } %>

        </div>
    </div>